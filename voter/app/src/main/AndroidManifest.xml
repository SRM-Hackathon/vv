<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.verification.voter">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Voter"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.verification.voter.MainActivity"
            android:label="Voter">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.verification.voter.UploadFile" android:label="Voter"/>
        <activity android:name="com.example.verification.voter.Signup" android:label="Voter"/>



    </application>

</manifest>
